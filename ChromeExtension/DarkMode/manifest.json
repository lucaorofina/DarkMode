{
    "manifest_version": 3,
    "name": "Auto Dark Mode",
    "version": "1.0",
    "description": "Automatically changes light websites to dark mode.",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
  }
  